{
	"name": "ddm-cli",
	"version": "2.0.4",
	"description": "Development stack for creating DDM visuals",
	"author": "Patrik Janosdeak @truesteps",
	"bin": {
		"ddm": "./bin/run"
	},
	"homepage": "https://github.com/nebe-app/ddm-cli",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"repository": "nebe-app/ddm-cli",
	"files": [
		"/bin",
		"/dist",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"dependencies": {
		"@frigus/is-hidden-file": "^1.0.1",
		"@oclif/core": "^1",
		"@oclif/plugin-autocomplete": "^1.2.0",
		"@oclif/plugin-help": "^5",
		"@oclif/plugin-not-found": "^2.3.1",
		"@oclif/plugin-update": "^3.0.0",
		"@oclif/plugin-warn-if-update-available": "^2.0.4",
		"@sentry/node": "^6.19.7",
		"adm-zip": "^0.5.9",
		"axios": "^0.27.2",
		"chalk": "^4.1.2",
		"chokidar": "^3.5.3",
		"cli-table": "^0.3.11",
		"configstore": "^5.0.0",
		"crypto": "^1.0.1",
		"express": "^4.18.0",
		"form-data": "^4.0.0",
		"fs": "^0.0.1-security",
		"fs-extra": "^10.1.0",
		"glob": "^8.0.1",
		"inquirer": "^8.2.2",
		"inquirer-search-list": "^1.2.6",
		"listr": "^0.14.3",
		"open": "^8.4.0",
		"path": "^0.12.7",
		"pdf2pic": "^2.1.4",
		"randomstring": "^1.2.2",
		"rimraf": "^3.0.2",
		"simple-git": "^3.7.1",
		"tcp-port-used": "^1.0.2",
		"which": "^2.0.2"
	},
	"devDependencies": {
		"@oclif/test": "^2",
		"@types/adm-zip": "^0.5.0",
		"@types/chai": "^4",
		"@types/cli-table": "^0.3.0",
		"@types/configstore": "^5.0.1",
		"@types/express": "^4.17.13",
		"@types/fs-extra": "^9.0.13",
		"@types/inquirer": "^8.2.1",
		"@types/listr": "^0.14.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.11.33",
		"@types/node-notifier": "^8.0.2",
		"@types/randomstring": "^1.1.8",
		"@types/rimraf": "^3.0.2",
		"@types/tcp-port-used": "^1.0.1",
		"@types/which": "^2.0.1",
		"chai": "^4",
		"eslint": "^7.32.0",
		"eslint-config-oclif": "^4",
		"eslint-config-oclif-typescript": "^1.0.2",
		"globby": "^11",
		"mocha": "^9",
		"oclif": "^2",
		"shx": "^0.3.3",
		"ts-node": "^10.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.6.4"
	},
	"oclif": {
		"bin": "ddm",
		"dirname": "ddm",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-not-found",
			"@oclif/plugin-update",
			"@oclif/plugin-warn-if-update-available",
			"@oclif/plugin-autocomplete"
		],
		"topicSeparator": " "
	},
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts --config .eslintrc",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "yarn lint",
		"prepack": "yarn build && oclif manifest && oclif readme",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"bugs": "https://github.com/nebe-app/ddm-cli/issues",
	"keywords": [
		"oclif"
	],
	"types": "dist/index.d.ts"
}
